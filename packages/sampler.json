{
    "name": "sampler",
    "author": "sqshq",
    "description": "Tool for shell commands execution, visualization and alerting. Configured with a simple YAML file.",
    "url": "https://github.com/sqshq/sampler.git",
    "license": "GPL-3.0",
    "language": "Go",
    "bin": {
        "in_source": [
            "sampler"
        ],
        "installed": [
            "sampler"
        ]
    },
    "commands": {
        "all": {
            "install": [
                "go build ."
            ],
            "update": [
                "go build ."
            ]
        }
    },
    "deps": {
        "all": [
            "go"
        ]
    }
}