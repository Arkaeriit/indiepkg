{
    "name": "pokeget",
    "author": "Talwat",
    "description": "Display cool sprites of pokemon in your terminal.",
    "url": "https://github.com/talwat/pokeget.git",
    "branch": "testing",
    "commands": {
        "all": {
            "install": [
                "make install PREFIX=:(PATH):"
            ],
            "uninstall": [
                "rm -v :(PATH):/bin/pokeget"
            ],
            "update": [
                "rm -v :(PATH):/bin/pokeget",
                "make install PREFIX=:(PATH):"
            ]
        }
    },
    "deps": {
        "all": [
            "bash",
            "curl",
            "make"
        ]
    },
    "config_paths": [
        ".config/pokeget"
    ]
}